{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "AppData/Logs/eolbot.log",
          "rollingInterval": "Month",
          "retainedFileCountLimit": 12
        }
      }
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=AppData/eolbot.db"
  },
  "Telegram": {
    "BotToken": "YOUR_BOT_TOKEN",
    "AdminChatId": 0
  },
  "Report": {
    "DaysToCover": 7,
    "MaxConcurrentMessages": 25
  },
  "EndOfLifeDate": {
    "ApiUrl": "https://endoflife.date/api/v1/",
    "ConnectionTimeout": 30
  },
  "LogReader": {
    "MaxLines": 25,
    "MaxLineLength": 160
  }
}
